{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "tsc-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "./packages/project-template-server"
      }
    },
    "build-dist": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run esm-script ./scripts/rollup.ts project-template-server"
      }
    },
    "build-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run esm-script ./scripts/rollup.ts server"
      }
    },
    "run-dist": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build-dist",
        "^build-dist"
      ],
      "options": {
        "command": "node ./dist/index.js",
        "cwd": "./packages/project-template-server"
      }
    },
    "run-debug": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build-dist",
        "^build-dist"
      ],
      "options": {
        "command": "node --inspect ./dist/index.js",
        "cwd": "./packages/project-template-server"
      }
    },
    "debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run esm-script ./scripts/watch.ts --emit project-template-server run-debug"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm run esm-script ./scripts/watch.ts --emit project-template-server run-dist"
      }
    }
  }
}